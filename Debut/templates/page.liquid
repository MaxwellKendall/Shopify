{% if page.url == "/pages/about-me" %}
<div class="page-width abt-me-page-width">
  {% else %}
  <div class="page-width">
  {%endif%}
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>
        {% if page.handle contains 'about-me' %}
      <div class="rte about-me">
        {% else %}
      <div class="rte">
		{% endif %}
        {{ page.content }}
      </div>
    </div>
  </div>
</div>
